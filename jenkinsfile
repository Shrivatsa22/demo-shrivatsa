node {
  try {
    stage('checkout') {
      checkout scm
    }
    stage('build') {
      sh cd build
	    sh cmake ..
	    sh make -j8
    }
    stage('compile') {
      echo "nothing to compile for hello.sh..."
    }
  }
  finally {
    stage('cleanup') {
      echo "doing some cleanup..."
    }
  }
}
